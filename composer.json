{
    "name": "affinitybridge/nationbuilder-api",
    "description": "Light PHP wrapper for the NationBuilder API v1",
    "license": "LGPL-3.0+", 
    "authors": [
        {
            "name": "Raphael Huefner",
            "email": "raphael@affinitybridge.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "justinrainbow/json-schema": "1.6.1"
    },
    "require-dev": {
        "ext-curl": "*",
        "php": ">=5.6.0",
        "phpunit/phpunit": "~4.0",
        "psr/http-message": "~1.0",
        "psr/log": "~1.0",
        "guzzlehttp/guzzle": "6.1.1",
        "guzzlehttp/psr7": "1.2.2"
    },
    "autoload": {
        "psr-4": {"Affinitybridge\\NationBuilder\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Affinitybridge\\NationBuilder\\Tests\\": "tests/"
        }
    }
}
